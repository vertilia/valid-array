{
  "name": "vertilia/valid-array",
  "description": "Object with automatic items validation based on standard filtering mechanism of php",
  "license": "BSD-2-Clause",
  "type": "library",
  "autoload": {
    "exclude-from-classmap": [
      "/tests/"
    ],
    "psr-4": {
      "Vertilia\\ValidArray\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.0"
    }
  },
  "require": {
    "php": "^7.4",
    "ext-filter": "*"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "^9.5"
  }
}
